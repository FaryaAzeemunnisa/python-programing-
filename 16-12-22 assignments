n=input()
length=len(n)
print(n.upper())
print(n.lower())

assig-2
***********************************
n=input()
c=0
for i in n:
    if i.isalpha():
        c=c+1
print(c)

assig-3
***********************************
n=input()
c=0
for i in n:
    if i.isdigit():
        c=c+1
print(c)
