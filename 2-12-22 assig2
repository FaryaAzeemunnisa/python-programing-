# way-1
def eligible(x):
    if x>=18:
        print("YES")
    else:
        print("NO")
        
x=int(input())
eligible(x)

#way-2
def eligible(x):
    if x>=18:
        return "YES"
    else:
        return "NO"
        
x=int(input())
print(eligible(x))

#way-3
def eligible(x):
    if x>=18:
        return True
    else:
        return False
        
x=int(input())
print('YES') if x>=18 else print('NO')
