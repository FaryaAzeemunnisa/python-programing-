assig-1
********************************
class year:
    def ly(self,m):
        if m%4==0 and m%100!=0:
            print("LEAP YEAR")
        elif m%100==0:
            print("LEAP YEAR")
        else:
            print("NOT LEAP YEAR")
m=int(input())
n=year()
n.ly(m)
