empno=int(input())
empname=input()
empdesignation=input()
bsal=int(input())
da=int(input())
ta=int(input())
hra=int(input())
netsal=bsal+da+ta+hra
if(netsal>100000):
    tax=netsal*10/100
elif(netsal>50000):
    tax=7
elif(netsal>40000):
    tax=4
elif(netsal>20000):
    tax=2
else:
    tax=0
print("the employee's tax:",tax,"%")
